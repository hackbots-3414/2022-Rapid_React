<!-- Logback Configuration from swerve_testing -->

<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date{mm:ss.SSS} [%thread] %highlight(%-5level) %logger{20} - %message %n</pattern>
        </encoder>
    </appender>

    <!-- <logger name="frc.robot.commands" level="INFO" /> -->
    <!-- <logger name="frc.robot.commands.EatBall" level="TRACE" /> -->
    <!-- <logger name="frc.robot.subsystems.vision.Pixy" level="TRACE" /> -->
    <!--logger name="frc.robot.commands.MrZPrintCommand" level="TRACE"/-->

    <!-- <logger name="frc.robot.subsystems" level="INFO" /> -->

    <!-- <logger name="frc.robot.subsystems.Drivetrain" level="TRACE"/> -->
    <!-- <logger name="frc.robot.commands.ConfigureReverseControls" level="TRACE"/> -->

    <root level="WARN">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>